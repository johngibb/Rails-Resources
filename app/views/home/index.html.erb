<% section "StackOverflow", "/stack_overflow_searches" do %>
   <dl class="form">
      <% @searches.each do |search| %>
   	   <dt><%= search.title %></dt>
   		<dd>
   		  	<form action="http://stackoverflow.com/search" method="get">
   				<input name="q" type="text" value="<%= search.prefix %> " />
   			</form>
   		</dd>
   	<% end %>
   </dl>
<% end %>

<% section "Links", "/links" do %>
   <%= render :partial => @links %>
<% end %>

<% section "Snippets", "/snippets" do %>
   <dl class="snippets">
      <% @snippets.each do |snippet| %>
         <dt><%= snippet.title %></dt>
         <dd>
            <pre><%= snippet.content %></pre>
         </dd>
      <% end %>
   </dl>
<% end %>